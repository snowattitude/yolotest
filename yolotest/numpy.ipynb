   "source": [
    "import numpy as np\n",
    "a = np.random.rand(1000000)    # 输出0—1之间长度为n的一维数组\n",
    "b = np.random.rand(1000000)\n",
    "c = np.dot(a,b)                # a^T*b"
   ]
